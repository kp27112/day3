
#include <stdio.h>
#include <stdlib.h>
#include <pthread.h>
#include <unistd.h>
#include <sys/types.h>


void *foo1(void* a)
{
    while(1)
       	sleep(1);
return 0;
}

void *foo2(void* a)
{
    while(1)
       	sleep(1);
return 0;
}


int main(void)
{
    pthread_t t_id1;
    pthread_create(&t_id1,NULL,foo1,NULL);

    pid_t p = fork();

    pthread_t t_id2;
    pthread_create(&t_id2,NULL,foo2,NULL);


   // pthread_join(t_id,0);

return 0;
}
